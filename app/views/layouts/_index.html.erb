<div x-ng-app="items_app">
  <div x-ng-controller="ItemCtrl">
    <ul class="example-orbit" data-orbit >
        <li class="" x-ng-repeat="favorite in favoritesAll track by $index">
          <img x-ng-src="{{ favorite.image_url }}" />
            <div class="orbit-caption">
              {{favorite.heading}}
              <br>
            </div>
            <br>
        </li>
    </ul>
  </div>
</div>