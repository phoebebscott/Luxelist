<!-- This is a carousel containing today's most popular items
It is viewable to logged-out and logged-in users -->
<div ng-controller="CarouselDemoCtrl">
  <div style="height: 305px">
    <carousel interval="myInterval">
      <slide ng-repeat="item in items" active="item.active">
        <img ng-src="{{item.images[0].full}}" style="margin:auto;">
        <div class="carousel-caption">
          <h4>{{item.heading}}</h4>
          <p>{{item.price}}</p>
          <p>{{item.location.city}}
        </div>
      </slide>
    </carousel>
  </div>
</div>
